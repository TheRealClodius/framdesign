{
  "name": "framdesign",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "NODE_ENV=development next dev --webpack",
    "dev:clean": "pkill -9 -f 'next dev' 2>/dev/null || true; lsof -ti:3000 | xargs kill -9 2>/dev/null || true; lsof -ti:8080 | xargs kill -9 2>/dev/null || true; sleep 1; echo 'âœ“ Cleaned up existing dev servers'",
    "dev:all": "npm run dev:clean && concurrently \"npm run dev\" \"npm run dev:voice\" --names \"FRONTEND,VOICE\" --prefix-colors \"cyan,yellow\"",
    "dev:voice": "cd voice-server && npm start",
    "build": "next build --webpack",
    "build:tools": "node tools/_build/tool-builder.js",
    "prebuild": "npm run build:tools",
    "prestart": "npm run build:tools",
    "start": "next start",
    "lint": "eslint",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:agent": "node scripts/text-agent-test.js"
  },
  "dependencies": {
    "@fontsource/google-sans-code": "^5.2.3",
    "@fontsource/google-sans-flex": "^5.2.1",
    "@google-cloud/storage": "^7.18.0",
    "@google/genai": "^1.35.0",
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^16.1.1",
    "@qdrant/js-client-rest": "^1.9.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dotenv": "^17.2.3",
    "gray-matter": "^4.0.3",
    "mermaid": "^11.12.2",
    "next": "16.1.1",
    "next-intl": "^4.7.0",
    "openai": "^6.15.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.6.0",
    "tiktoken": "^1.0.22",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
